  "main": "index.js",
  "scripts": {
    "api": "nodemon mocks/server.js",
    "build": "yarn clear && webpack",
    "build:prod": "yarn clear && webpack --config configs/webpack/production.config.js --progress --profile --bail",
    "clear": "rimraf aot dist",
    "e2e": "npm-run-all -p -r start:prod protractor",
    "postinstall": "webdriver-manager update",
    "protractor": "protractor configs/protractor/config.js",
    "server:dev": "npm-run-all -p -r api webpack-dev-server",
    "server:prod": "http-server dist -p 9001 -c-1 --cors",
    "test": "karma start configs/karma/config.js",
    "webpack-dev-server": "webpack-dev-server --inline --progress"
  },
